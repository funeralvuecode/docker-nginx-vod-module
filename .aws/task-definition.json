{
    "containerDefinitions": [
        {
            "name": "vod",
            "image": "fvue-stng-stream-ecr:latest",
            "cpu": 1024,
            "memory": 2048,
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "AWS_REGION",
                    "value": "us-east-2"
                },
                {
                    "name": "AWS_SECRET",
                    "value": ""
                },
                {
                    "name": "AWS_ACCESS_KEY",
                    "value": ""
                },
                {
                    "name": "AWS_S3_BUCKET",
                    "value": ""
                },
                {
                    "name": "PORT",
                    "value": "8080"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/aws/ecs/fvue-stng-stream-vod",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "fvue-stng-stream"
                }
            },
            "systemControls": []
        }
    ],
    "family": "fvue-stng-stream-vod",
    "executionRoleArn": "arn:aws:iam::900810998733:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "2048",
    "memory": "4096",
    "tags": []
}
